            <string>///Kill off stragglers, Survivors reproduce
with (objBlob)
{
    age++;
}
with (objBlob)
{
    if (age &gt; 0)
    {
        if (blob_awake) //If at the end of the day they don't have food or are still searching for food
        {
            instance_destroy();
        }
        else
        {
            var num_offspring = blob_food_count * ds_map_find_value(blob_stats, "offspring_food_ratio");
            for (var i = 0; i &lt; num_offspring; i++)
            {
                var offspr = instance_create(x, y, objBlob);
                ds_map_copy(offspr.blob_stats, blob_stats);
                var i_stats = ds_map_find_first(offspr.blob_stats);
                while (!is_undefined(i_stats))
                {
                    var stat = ds_map_find_value(offspr.blob_stats, i_stats);
                    var mutation_amt = random(mutation_percent_change * 2) - mutation_percent_change;  //Calculate a random amount this stat will mutate
                    mutation_amt = ((mutation_amt + 100) / 100);
                    stat = stat * mutation_amt;                             //Mutate the stat
                    ds_map_replace(offspr.blob_stats, i_stats, stat);       //Save the mutated stat back into the offspring's gene pool
                    i_stats = ds_map_find_next(offspr.blob_stats, i_stats);   //Get the next stat
                }
                offspr.blob_awake = true;
            }
            blob_food_count = 0;
            blob_awake = true;
        }
    }
}
with (objGreens)
